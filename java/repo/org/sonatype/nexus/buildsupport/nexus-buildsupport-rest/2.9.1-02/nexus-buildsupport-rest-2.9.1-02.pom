        <groupId>org.sonatype.sisu.siesta</groupId>
        <artifactId>siesta-jackson</artifactId>
        <version>${siesta.version}</version>
      </dependency>

      <dependency>
        <groupId>org.sonatype.sisu.jacksbee</groupId>
        <artifactId>jacksbee-runtime</artifactId>
        <version>1.3</version>
