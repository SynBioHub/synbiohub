PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX sbh: <http://wiki.synbiohub.org/wiki/Terms/synbiohub#>

DELETE WHERE {
    <$topLevel> sbh:mutableDescription ?desc .
    <$topLevel> dcterms:modified ?modified .
}
;
INSERT DATA {
    <$topLevel> sbh:mutableDescription $desc .
    <$topLevel> dcterms:modified $modified .
} 


